{
	"name": "nest-react",
	"author": "svetaet",
	"license": "MIT",
	"browserslist": "> 0.5% in alt-EU, > 0.5% in alt-NA, > 0.5% in RU, not ie > 0, not chrome 49, not edge 18",
	"scripts": {
		"open:dev": "start http://localhost:3001",
		"open:prod": "start http://localhost:3000",
		"run": "node dist/server/main",
		"up": "docker-compose up -d --build",
		"dev:client": "webpack-dev-server --mode=development --config webpack/client.js",
		"dev:server": "webpack --watch --mode=development --config webpack/server.js",
		"build:client": "webpack --mode=production --config webpack/client.js",
		"build:server": "webpack --mode=production --config webpack/server.js",
		"build": "yarn build:client && yarn build:server",
		"lint": "eslint \"{src,test,config}/**/*.{ts,tsx,js,json}\"",
		"lint:fix": "yarn lint --fix",
		"types": "tsc --watch",
		"test": "jest",
		"test:cov": "jest --coverage",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"update": "yarn upgrade-interactive --latest"
	},
	"devDependencies": {
		"@babel/core": "^7.10.3",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
